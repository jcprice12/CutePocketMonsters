
<h1 id="registerHeader">REGISTER TO START BUILDING YOUR TEAM</h1>

<form class="myForm" id="registerUserForm" action="/register" method="post">
	<div>
        <label>USERNAME</label><br>
        <input class="loginInput" id="usernameInput" type="text" name="username"/><br/>
	</div>
    <div>
        <label>EMAIL</label><br>
        <input class="loginInput" id="emailInput" type="text" name="email"/><br/>
	</div>
	<div>
	    <label>PASSWORD</label><br>
	    <input class="loginInput" id="passwordInput" type="password" name="password"/>
	</div>
	<div class="submit">
		<span class="carrot3">
        	<i class="fa fa-caret-right" aria-hidden="true"></i>
    	</span>
		SUBMIT
	</div>
</form>
<script src="assets/javascript/register.js"></script>